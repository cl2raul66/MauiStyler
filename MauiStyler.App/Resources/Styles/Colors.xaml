<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <!--  PRINCIPAL  -->
    <Color x:Key="Primary">#FF512BD4</Color>
    <Color x:Key="Secondary">#FF2B0B98</Color>
    <Color x:Key="Accent">#FF2B0B98</Color>
    <!--  SEMANTIC  -->
    <Color x:Key="Error">#FFFF0000</Color>
    <Color x:Key="Success">#FF00FF00</Color>
    <Color x:Key="Warning">#FFFFFF00</Color>
    <!--  NEUTRAL  -->
    <Color x:Key="Foreground">#FFF7F5FF</Color>
    <Color x:Key="Background">#FF23135E</Color>
    <Color x:Key="Gray250">#FFE1E1E1</Color>
    <Color x:Key="Gray500">#FFACACAC</Color>
    <Color x:Key="Gray750">#FF6E6E6E</Color>
</ResourceDictionary>
